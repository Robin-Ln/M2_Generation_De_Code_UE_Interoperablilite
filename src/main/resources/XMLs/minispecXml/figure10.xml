<?fr.ubo.m2tiil.louarn.xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE modeleMinispec SYSTEM "minispec.dtd">

<modeleMinispec name="fr.ubo.m2tiil.louarn.minispecEnMinispec.modele.autre">

    <entity name="Satelite">

        <attributeMinispec name="nom">
            <typeElement type="String"/>
        </attributeMinispec>

        <attributeMinispec name="parent">
            <typeElement type="Flotte"/>
        </attributeMinispec>

    </entity>

    <entity name="Flotte">
        <attributeMinispec name="satelites">
            <collection typeCollection="List" max="10">
                <typeElement type="Satelite"/>
            </collection>
        </attributeMinispec>
    </entity>

</modeleMinispec>
